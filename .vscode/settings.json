{
  "debug.openExplorerOnEnd": true,
  "editor.tabSize": 2,
  "editor.rulers": [
    100
  ],
  "editor.autoIndent": "full",
  "editor.cursorBlinking": "solid",
  "editor.formatOnType": false,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.codeActionsOnSave": {
    "source.organizeImports": false,
    "source.fixAll.tslint": true,
  },
  "explorer.openEditors.visible": 1,
  "files.trimTrailingWhitespace": true,
  "files.autoSave": "onFocusChange",
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "npm.enableScriptExplorer": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "workbench.iconTheme": "material-icon-theme",
  "auto-close-tag.SublimeText3Mode": true,
  "html.autoClosingTags": true,
  "peacock.affectActivityBar": true,
  "peacock.affectStatusBar": true,
  "peacock.affectTitleBar": false,
  "workbench.colorCustomizations": {
    "activityBar.background": "#8ba1a5",
    "activityBar.activeBorder": "#a73896",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#a73896",
    "activityBarBadge.foreground": "#e7e7e7",
    "statusBar.background": "#6e898f",
    "statusBarItem.hoverBackground": "#586d72",
    "statusBar.foreground": "#15202b",
    "activityBar.activeBackground": "#8ba1a5",
    "statusBar.border": "#6e898f"
  },
  "peacock.color": "#6e898f",
  "gitlens.menus": {
    "editorGroup": false
  },
  "ng-evergreen.upgradeChannel": "Latest"
}
